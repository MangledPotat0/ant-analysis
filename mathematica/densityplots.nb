          RowBox[{"b", ",", "1", ",", 
           RowBox[{"dim1", " ", "dim2"}]}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"nAV", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"Sum", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"hist", "\[LeftDoubleBracket]", 
             RowBox[{"n", ",", "b"}], "\[RightDoubleBracket]"}], 
            RowBox[{"(", 
             RowBox[{"n", "-", "1"}], ")"}]}], ",", 
